#include<bits/stdc++.h>
using namespace std;
int ileSpelniaWarunek(int tab[], int rozmiar)
{	
	int licznik = 0;
	for(int i=0; i<rozmiar; i++)
	{
		if(tab[i]>3) licznik++;
	}
	return licznik;
}
int main()
{	int rozmiar = 10;
	int tab[rozmiar] = {1,5,8,3,67,9,3,45,1,3};
	cout<<ileSpelniaWarunek(tab, rozmiar);
	
}
